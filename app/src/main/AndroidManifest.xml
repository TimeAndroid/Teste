<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kaio.findpackrecycler">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitys.SplashScreen">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.EnviadasActivity" />
        <activity android:name=".Activitys.PendentesActivity" />
        <activity android:name=".Activitys.EntregasActivity" />
        <activity android:name=".Activitys.NovaEncomenda" />
        <activity android:name=".Activitys.MainActivity"></activity>

    </application>

</manifest>